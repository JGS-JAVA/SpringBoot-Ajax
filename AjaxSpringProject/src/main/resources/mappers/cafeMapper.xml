<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kh.AjaxSpringProject.mapper.CafeMapper">
    <select id="getAllCafe" resultType="com.kh.AjaxSpringProject.dto.Cafe">
        select *
        from cafes


    </select>
    <select id="getCafeById">
        select *
        from cafes
        where id = #{id}
    </select>
</mapper>